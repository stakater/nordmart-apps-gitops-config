apiVersion: tenantoperator.stakater.com/v1beta1
kind: Tenant
metadata:
  name: demo
spec:
  quota: medium
  sandbox: true
  owners:
    users:
      - Gabbar_Owner@azuretestrootstakater.onmicrosoft.com
    groups:
      - azure-gabbar-owner
  editors:
    users:
      - Gabbar_Editor@azuretestrootstakater.onmicrosoft.com
    groups:
      - azure-gabbar-editor
  viewers:
    groups:
      - azure-gabbar-viewer
  argocd:
    sourceRepos:
      - 'https://github.com/stakater/gitops-config-template'
      - 'https://nexus-helm-stakater-nexus.apps.binero-test.8sdzwd1l.kubeapp.cloud/repository/helm-charts/'
  templateInstances:
  - spec:
      template: docker-secret-template
      parameters:
        - name: ENCRYPTED_DOCKER_CONFIG
          value: 
  namespaces:
  - dev
  - prod
